#ifndef IEditable_H
#define IEditable_H

///@brief Интерфейс поддержки функций правки
class IEditable
{
public:
  virtual ~IEditable ( ) { } 
  virtual void undo () {} ///< Отменить
  virtual void redo () {} ///< Вернуть
  virtual void cut () {}  ///< Вырезать
  virtual void copy () {} ///< Копировать
  virtual void paste () {}///< Вставить
  virtual void specialPaste () {}///< Специальная вставка
};

#endif // IEditable_H

